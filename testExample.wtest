import example.*

describe "group of tests for pepita" {
  method initialize(){
    tito.consumir(1,whisky)
  }
  
  method consumirTerere(cantidad,bebida){
    tito.consumir(cantidad,bebida)
  }
  test "test1" {
    assert.equals(70, tito.peso())
  }
  test "test2" {
    self.consumirTerere(10,terere)
    assert.equals(7, tito.velocidad(10))
  }

}